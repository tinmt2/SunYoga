{
  "name": "mobile-yoga-payments",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "cross-env EXPO_USE_HERMES=0 EXPO_WEB_USE_HERMES=0 expo start --web",
    "web:export": "expo export --platform web --output-dir dist/web-build --dump-sourcemap",
    "desktop:dev": "cd electron && npm run dev",
    "desktop:build": "npm run web:export && cd electron && npm run build",
    "desktop:build:win": "npm run web:export && cd electron && npm run build:win",
    "desktop:build:mac": "npm run web:export && cd electron && npm run build:mac",
    "desktop:build:linux": "npm run web:export && cd electron && npm run build:linux"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.8.2",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "date-fns": "^3.6.0",
    "expo": "~54.0.22",
    "expo-status-bar": "~3.0.8",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.75.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-web": "0.19.12",
    "zustand": "^5.0.8"
  },
  "private": true,
  "devDependencies": {
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/types": "^7.28.5",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "cross-env": "^10.1.0",
    "electron": "^39.1.2",
    "electron-builder": "^26.0.12"
  }
}
